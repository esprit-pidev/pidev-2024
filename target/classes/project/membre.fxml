<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="85.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.GerermembresController">
    <VBox fx:id="affmem" layoutX="14.0" layoutY="16.0" prefHeight="53.0" prefWidth="329.0">
        <children>
            <Label fx:id="mail" text="Label">
                <VBox.margin>
                    <Insets bottom="10.0" top="-10.0" />
                </VBox.margin>
                <font>
                    <Font name="Baskerville Old Face" size="14.0" />
                </font>
            </Label>
            <ImageView fx:id="delmem" fitHeight="24.0" fitWidth="23.0" onMouseClicked="#supprimermembre" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@images/delete.png" />
                </image>
                <VBox.margin>
                    <Insets left="300.0" />
                </VBox.margin>
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
            </ImageView>
        </children>
        <padding>
            <Insets left="10.0" top="10.0" />
        </padding>
    </VBox>
</AnchorPane>
